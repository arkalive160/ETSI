<p id="notice"><%= notice %></p>

<h1>Componente Practicos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre practico</th>
      <th>Total horas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @componente_practicos.each do |componente_practico| %>
      <tr>
        <td><%= componente_practico.nombre_practico %></td>
        <td><%= componente_practico.total_horas %></td>
        <td><%= link_to 'Show', componente_practico %></td>
        <td><%= link_to 'Edit', edit_componente_practico_path(componente_practico) %></td>
        <td><%= link_to 'Destroy', componente_practico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Componente Practico', new_componente_practico_path %>
