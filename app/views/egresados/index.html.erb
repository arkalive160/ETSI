<p id="notice"><%= notice %></p>

<h1>Egresados</h1>

<table>
  <thead>
    <tr>
      <th>Es empleado</th>
      <th>Caracter empleo</th>
      <th>Fecha egreso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @egresados.each do |egresado| %>
      <tr>
        <td><%= egresado.es_empleado %></td>
        <td><%= egresado.caracter_empleo %></td>
        <td><%= egresado.fecha_egreso %></td>
        <td><%= link_to 'Show', egresado %></td>
        <td><%= link_to 'Edit', edit_egresado_path(egresado) %></td>
        <td><%= link_to 'Destroy', egresado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Egresado', new_egresado_path %>
